{
  "functions": [
    {
      "source": "src",
      "codebase": "default",
      "ignore": [
        "venv",
        ".git",
        "firebase-debug.log",
        "firebase-debug.*.log",
        "*.local",
        "**/tests/**",
        "**/__pycache__/**",
        "**/*.pyc"
      ]
    }
  ],
  "emulators": {
    "functions": {
      "port": 5002
    },
    "ui": {
      "enabled": false
    },
    "singleProjectMode": true,
    "pubsub": {
      "port": 8085
    }
  }
}